@using BlogApp.Models
@model IEnumerable<Tag>

@{
    ViewBag.Title = "TagBox";
}

<div class="panel panel-primary">
    <div class="panel-heading tagbox-custom-header">
        POPULAR TOPICS
    </div>
    <div class="panel-body">
        @foreach (var item in Model)
            {
            <span class="btn btn-default tag-custom" onclick="location.href='@Url.Action("SearchBlogByTag","Blogs",new { id = item.Id,title = item.Title })'">
                @item.Title - @item.Famous
            </span>
        }
    </div>
    @*<div class="panel-footer">
        This is tag section
    </div>*@
</div>
